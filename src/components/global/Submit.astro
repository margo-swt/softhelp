---
import BaseLayout from "../layouts/BaseLayout.astro";
import Submit from "../components/global/Submit.astro";


---


<AstroSeo
  title="Soft Help Tips Home"
  description="Your site description goes here"
  canonical="https://softhelp.tips"
  openGraph={{
    url: "https://softhelp.tips",
    title: "Soft Help Tips",
    description: "Elevate your QA strategies with our help",
    images: [
      {
        url: "/assets/opengraph-twitter.png",
        width: 1200,
        height: 630,
        alt: "Social open graph",
        type: "image/png",
      },
      {
        url: "/assets/opengraph-twitter.png",
        width: 1200,
        height: 630,
        alt: "Twitter open graph",
        type: "image/png",
      },
      { url: "/assets/opengraph-twitter.png" },
      { url: "/assets/opengraph-twitter.png" },
    ],
    site_name: "YourSitesName",
  }}
  twitter={{
    handle: "@TwitterHandle",
    site: "@site",
    cardType: "summary_large_image",
  }}
/>
<BaseLayout>
  <Submit />
</BaseLayout>
